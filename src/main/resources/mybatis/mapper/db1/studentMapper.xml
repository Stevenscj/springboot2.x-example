<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mgtv.demo.dao.mapper.db1.Student1Mapper">
    <sql id="Base_Column_List">
        id, stud_name, stud_age, stud_sex, create_time, update_time
    </sql>
    <select id="listStudentPage" resultType="Student1DO">
        select
        <include refid="Base_Column_List"/>
        from student
        order by id
    </select>

    <select id="selectByName" resultType="Student1DO">
        select
        <include refid="Base_Column_List"/>
        from student
        where stud_name like CONCAT('%',#{studName},'%')
    </select>
</mapper>